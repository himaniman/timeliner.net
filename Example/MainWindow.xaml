<Window x:Class="Example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:tl="clr-namespace:TimelinerNet;assembly=TimelinerNet"
        xmlns:local="clr-namespace:Example"
        mc:Ignorable="d"
        Title="Example" Height="300" Width="600">
    <StackPanel>
        <tl:Timeliner Items="{Binding Items, Mode=OneWay}" FontSize="12" Margin="4"/>
        <TextBlock Foreground="White" Text="asd"/>
        <Button Content="another element"/>
        <ListView>
            <ListView.View>
                <GridView>
                    <GridView.Columns>
                        <GridViewColumn Header="another element" Width="200"/>
                    </GridView.Columns>
                </GridView>
            </ListView.View>
            <ListViewItem Content="another element"/>
        </ListView>
    </StackPanel>
</Window>
